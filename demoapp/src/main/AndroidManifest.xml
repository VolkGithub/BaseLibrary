        </activity>
        <activity
            android:name=".demo.launchmode.LaunchModeActivity3"
            android:label="@string/launch_mode">

        </activity>
        <activity
            android:name=".demo.launchmode.LaunchModeActivity4"
            android:label="@string/launch_mode"
            android:launchMode="standard">

        </activity>
