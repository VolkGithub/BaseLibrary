        <activity
            android:name=".activity.DatabaseActivity"
            android:label="@string/database"></activity>
        <activity
            android:name=".activity.AddFriendActivity"
            android:label="@string/addcontacts"></activity>
        <activity
            android:name="com.kevin.building.activity.UserListActivity"
            android:label="@string/user_list"></activity>
        <activity
            android:name=".activity.RegistActivity"
            android:label="@string/register"></activity>
        <activity
            android:name=".activity.UserInfoActivity"
            android:label="@string/user_info"></activity>
        <activity
            android:name=".activity.LogoutActivity"
            android:label="@string/logout"></activity>
        <activity
            android:name=".demo.draggridview.DragGridViewActivity"

            android:label="@string/drag_gridveiw_demo"></activity>

        <activity
            android:name=".activity.SearchActivity"

            android:label="@string/search_demo"></activity>

        <activity
            android:name=".demo.launchmode.LaunchModeActivity1"

